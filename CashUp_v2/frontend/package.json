{
  "name": "cashup-frontend",
  "version": "2.0.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^5.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "antd": "^5.11.0",
    "axios": "^1.3.0",
    "classnames": "^2.3.2",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "image-webpack-loader": "^7.0.1",
    "moment": "^2.30.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.8.0",
    "react-scripts": "5.0.1",
    "recharts": "^3.1.2"
  },
  "scripts": {
    "start": "webpack serve --config webpack.config.js --env development",
    "build": "webpack --config webpack.config.js --env production",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:e2e": "cypress run",
    "test:e2e:headed": "cypress run --headed",
    "test:component": "cypress run --component",
    "test:component:headed": "cypress run --component --headed",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "format:check": "prettier --check src/**/*.{ts,tsx}",
    "clean": "rm -rf dist node_modules build",
    "setup:cypress": "node node_modules/cypress/bin/cypress install",
    "analyze": "webpack-bundle-analyzer build/js/*.js",
    "dev": "npm-run-all --parallel start lint:watch",
    "lint:watch": "eslint src --ext .ts,.tsx --watch",
    "type-check:watch": "tsc --noEmit --watch",
    "build:analyze": "NODE_ENV=production npm run build && npm run analyze"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.23.3",
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "babel-jest": "^30.1.2",
    "babel-loader": "^9.1.3",
    "classnames": "^2.3.2",
    "clean-webpack-plugin": "^4.0.0",
    "compression-webpack-plugin": "^10.0.0",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "identity-obj-proxy": "^3.0.0",
    "image-minimizer-webpack-plugin": "^3.0.0",
    "imagemin": "^9.0.1",
    "imagemin-webp": "^8.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^1.0.0",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "mini-css-extract-plugin": "^2.7.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.32",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^9.3.0",
    "style-loader": "^3.3.3",
    "css-loader": "^6.8.1",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-bundle-analyzer": "^4.9.1",
    "terser-webpack-plugin": "^5.3.9",
    "ts-jest": "^29.4.1",
    "typescript": "^5.9.2",
    "cypress": "^12.10.0",
    "@cypress/webpack-preprocessor": "^5.11.0",
    "cypress-mochawesome-reporter": "^3.4.0",
    "cypress-grep": "^2.0.0"
  }
}
